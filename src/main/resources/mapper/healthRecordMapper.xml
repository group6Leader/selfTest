<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 회원정보 처리 SQL -->
<mapper namespace="com.seltest.www.dao.HealthRecordMapper">

	<insert id="insertHealthRecord" parameterType="healthRecord">
		insert into customer (

		dia_Num
		,dis_Name
		,final_Diagnosis
		,dis_Category
		,first_Date
		,dia_Date
		,care_Opinions
		,ect
		,usage
		,issue_Date
		,hos_Name
		,hos_Address
		,hos_Phone
		,cust_Num
		,dia_del

		
		)values(
		
		dia_Num_sequence.nextval
		,#{dis_Name}
		,#{final_Diagnosis}
		,#{dis_Category}
		,#{first_Date}
		,#{dia_Date}
		,#{care_Opinions}
		,#{ect}
		,#{usage}
		,#{issue_Date}
		,#{hos_Name}
		,#{hos_Address}
		,#{hos_Phone}
		,#{cust_Num}
		,${dia_del}
		
		)
	</insert>
	
	
	


</mapper>