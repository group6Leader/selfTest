<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.seltest.www.dao.HealthRecordMapper">


	<insert id="insertHealthRecord" parameterType="healthRecord">
		insert into healthRecord (

		dia_Num
		,dis_Name
		,final_Diagnosis
		,dis_Category
		,first_Date
		,dia_Date
		,care_Opinions
		,ect
		,usage
		,issue_Date
		,hos_Name
		,hos_Address
		,hos_Phone
		,cust_Num
		,dia_del

		
		)values(
		
		dia_Num_sequence.nextval
		,#{dis_Name}
		,#{final_Diagnosis}
		,#{dis_Category}
		,#{first_Date}
		,#{dia_Date}
		,#{care_Opinions}
		,#{ect}
		,#{usage}
		,#{issue_Date}
		,#{hos_Name}
		,#{hos_Address}
		,#{hos_Phone}
		,#{cust_Num}
		,#{dia_del}
		
		)
	</insert>
	
	<select id="searchDisCode" parameterType="String" resultType="disCode">

		select * 
		from diseaseCode 
		where dis_code like '%' || #{discode} || '%'
		order by dis_code

	</select>
	


</mapper>